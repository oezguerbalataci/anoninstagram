{
	"manifest_version": 2,

	"name": "Seen Instagram stories anonymously",
	"description": "Seen Instagram stories anonymously",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [ "https://*.instagram.com/*", "webRequest", "webRequestBlocking" ],
	"background": {
		"scripts": ["./background.js"]
	}
}